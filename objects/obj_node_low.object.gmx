                ds_grid_set(game_controller.grid_powered, 
                            i, j, game_controller.TILE_POWERED_TRUE);
            }
        }
    }
    else
    {
        for (var i=start_x; i&lt;=end_x; i++)
        {
            for (var j=start_y; j&lt;=end_y; j++)
            {
                ds_grid_set(game_controller.grid_power_delivered, 
                            i, j, 0);
                ds_grid_set(game_controller.grid_powered, 
                            i, j, game_controller.TILE_POWERED_FALSE);
            }
        }
        game_controller.outage = true;
